{
  "name": "server",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development bun run --hot src/index.ts",
    "build": "NODE_ENV=production bun build src/index.ts --outdir dist --target bun",
    "start": "NODE_ENV=production bun run dist/index.js",
    "studio": "prisma studio --port 3001 --hostname 0.0.0.0",
    "postinstall": "prisma generate",
    "typecheck": "tsc --noEmit",
    "seed": "bun run prisma/seed.ts",
    "import-csv": "bun run scripts/import-csv.ts",
    "test-csv": "bun run scripts/test-csv-parse.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "prisma": {
    "seed": "bun run prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/google-libphonenumber": "^7.4.30",
    "prisma": "^6.17.1"
  },
  "peerDependencies": {
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@better-auth/expo": "1.3.24",
    "@hono/node-server": "1.12.0",
    "@hono/zod-validator": "0.7.3",
    "@prisma/adapter-better-sqlite3": "^6.17.1",
    "@prisma/client": "^6.17.1",
    "better-auth": "1.3.24",
    "fuse.js": "^7.1.0",
    "google-libphonenumber": "^3.2.43",
    "hono": "4.6.0",
    "twilio": "^5.10.7",
    "zod": "4.1.11"
  }
}
